{
  "name": "pm-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "bun --hot --no-clear-screen \"src/server/app.js\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "start:all": "concurrently \"bun run dev\" \"bun run server\"",
    "oauth-server": "node src/scripts/oauth-callback-server.js",
    "tauri": "tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@tauri-apps/plugin-deep-link": "~2",
    "@tauri-apps/plugin-process": "~2",
    "@tauri-apps/plugin-updater": "~2",
    "dotenv": "^17.2.3",
    "pinia": "3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue3-google-login": "^2.0.34",
    "vuetify": "^3.10.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@tauri-apps/cli": "^2.8.4",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.4.0",
    "globals": "^16.4.0",
    "happy-dom": "^15.11.7",
    "jsdom": "^25.0.1",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4"
  }
}
